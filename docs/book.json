{
  "description": "API for cap-client, the CLI tool of CERN Analysis Preservation",
  "plugins": [
    "insert-logo",
    "back-to-top-button",
    "copy-code-button",
    "anchorjs",
    "github",
    "-sharing",
    "codeblock-filename",
    "terminal",
    "richquotes"
  ],
  "variables": {
    "themeFexa": {
      "nav": [
        {
          "url": "https://github.com/cernanalysispreservation/cap-client",
          "target": "_blank",
          "name": "Github"
        }
      ]
    }
  },
  "pluginsConfig": {
    "insert-logo": {
      "url": "https://awesome-workshop.github.io/cap-cms/fig/banner.png",
      "style": "width: 100%;"
    },
    "anchorjs": {
      "placement": "left",
      "visible": "always"
    },
    "github": {
      "url": "https://github.com/cernanalysispreservation/cap-client"
    },
    "theme-api": {
      "languages": [
        {
          "lang": "python",
          "name": "Python"
        }
      ]
    },
    "terminal": {
      "copyButtons": true,
      "fade": true,
      "style": "flat"
    },
    "theme-fexa": {
      "search-placeholder": "Search the docs",
      "logo": "./cap-logo-dark.png",
      "favicon": "./favicon.ico"
    },
		"richquotes": {
			"star": {
				"alert": "warning",
				"picto": "fa-star"
			}
		}
  }
}